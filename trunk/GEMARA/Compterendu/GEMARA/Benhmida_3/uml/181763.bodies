!!!135171.cpp!!!	readAllButtons() : int
	int buttonsState[5];
	int result = 0;
	
	for(int i = 0; i < 5; i++) //stocke l'Ã©tat des 5 boutons dans un tableau
	{
		Button tmp = this->buttons.at(i);
		buttonsState[i] =  tmp.getState();
	}
	int i = 0;
	
	for(int j = 1; j<=16;j=j*2)
	{
		result = result + (j * buttonsState[i]);
		i++;
	}
	return result;
